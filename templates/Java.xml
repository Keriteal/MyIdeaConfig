      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="pricons" value="public $CLASS_NAME$(String[] args){&#10;  $END$&#10;}" description="private 构造器" toReformat="false" toShortenFQNames="true">
